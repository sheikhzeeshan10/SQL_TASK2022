CREATE TRIGGER tracks_audit_update
AFTER UPDATE ON tracks
	BEGIN
		INSERT INTO tracks_audit_log (operation, datetime, username, old_value, new_value)
		VALUES (&quot;UPDATE&quot;,datetime(),&quot;Admin&quot;,OLD.name,NEW.name);
	END;
	
